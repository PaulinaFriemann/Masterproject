  file = {C\:\\Users\\Pauli\\Zotero\\storage\\2WPVANF8\\Watkins_Rose_2013_Evaluating the performance of individual-based animal movement models in novel.pdf;C\:\\Users\\Pauli\\Zotero\\storage\\9QR3RIJ4\\S0304380012005480.html},
