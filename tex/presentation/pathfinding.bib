@article{valdeolivas2019,
  title = {Random Walk with Restart on Multiplex and Heterogeneous Biological Networks},
  author = {Valdeolivas, Alberto and Tichit, Laurent and Navarro, Claire and Perrin, Sophie and Odelin, Ga{\"e}lle and Levy, Nicolas and Cau, Pierre and Remy, Elisabeth and Baudot, Ana{\"i}s},
  year = {2019},
